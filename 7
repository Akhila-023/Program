package org;

import java.util.Scanner;

public class Fouradd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
System.out.println("enter 4 num");
		Scanner sc=new Scanner(System.in);
		int i=sc.nextInt();
		int j=sc.nextInt();
		int k=sc.nextInt();
		int l =sc.nextInt();
		System.out.println(+i+""+j+""+k+""+l);
	int	c=add(i,j,k,l);
	System.out.println("The sum of all digits"+c);
	}

 static int add(int a, int b, int d, int e) {
		// TODO Auto-generated method stub
		return  a+b+e+d;
	}
}
		
	

